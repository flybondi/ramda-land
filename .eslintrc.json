{
  "plugins": ["ramda"],
  "extends": ["standard", "plugin:ramda/recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "jest": true
  },
  "rules": {
    "semi": [2, "always"],
    "no-extra-semi": 2,
    "strict": ["error", "global"]
  }
}
