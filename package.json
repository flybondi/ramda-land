{
  "name": "@flybondi/ramda-land",
  "version": "4.0.4",
  "description": "A collection of reusable functions for ramda",
  "keywords": [
    "ramda",
    "function",
    "functions",
    "functional",
    "fp",
    "flybondi",
    "land",
    "utils"
  ],
  "homepage": "https://github.com/flybondi/ramda-land#readme",
  "bugs": {
    "url": "https://github.com/flybondi/ramda-land/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flybondi/ramda-land"
  },
  "license": "MIT",
  "author": "Flybondi <dev@flybondi.com> (https://flybondi.com)",
  "sideEffects": false,
  "unpkg": "dist/ramda-land.js",
  "module": "es/index.js",
  "main": "lib/index.js",
  "files": [
    "es",
    "lib",
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./es/index.js",
      "default": "./lib/index.js"
    },
    "./es": "./es/",
    "./lib": "./lib"
  },
  "scripts": {
    "clean": "rimraf es lib dist",
    "build:es": "cross-env BABEL_ENV=es babel --ignore '**/*.test.js' index.js src/ --out-dir es",
    "build:cjs": "cross-env BABEL_ENV=commonjs babel --ignore '**/*.test.js' index.js src --out-dir lib",
    "build:umd": "rollup -c",
    "build": "yarn clean && yarn build:es && yarn build:cjs && yarn build:umd",
    "prepack": "yarn build",
    "docs": "jsdoc -c jsdoc.json",
    "lint": "eslint index.js src/",
    "prettier": "prettier --write src/ index.js",
    "test": "jest --env=node --colors --bail --detectOpenHandles --errorOnDeprecated --noStackTrace src/",
    "test:coverage": "yarn test --coverage --coverageDirectory .coverage",
    "test:watch": "yarn test --watchAll"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "yarn test"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "yn": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@types/jest": "^26.0.20",
    "@types/ramda": "^0.27.34",
    "cross-env": "^7.0.3",
    "docdash": "^1.2.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-standard": "^4.1.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ramda": "^0.27.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.36.2",
    "rollup-plugin-clean": "^1.0.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "peerDependencies": {
    "ramda": ">=0.26.1"
  },
  "engines": {
    "node": ">=8"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "npx run-s docs"
        }
      ],
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "build",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md",
            "docs"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
